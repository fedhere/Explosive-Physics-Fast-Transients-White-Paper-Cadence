@article{Bianco2011,
  author={F. B. Bianco and D. A. Howell and M. Sullivan and A. Conley and D. Kasen and S. GonzÃ¡lez-GaitÃ¡n and J. Guy and P. Astier and C.
Balland and R. G. Carlberg and D. Fouchez and N. Fourmanoit and D. Hardin and I. Hook and C. Lidman and R. Pain and N.
Palanque-Delabrouille and S. Perlmutter and K. M. Perrett and C. J. Pritchet and N. Regnault and J. Rich and V.
Ruhlmann-Kleider},
  title={Constraining Type Ia Supernovae Progenitors from Three Years of Supernova Legacy Survey Data},
  journal={The Astrophysical Journal},
  volume={741},
  number={1},
  pages={20},
  url={http://stacks.iop.org/0004-637X/741/i=1/a=20},
  year={2011},
  abstract={While it is generally accepted that Type Ia supernovae are the result of the explosion of a carbon-oxygen white dwarf accreting mass in a binary system, the details of their genesis still elude us, and the nature of the binary companion is uncertain. Kasen points out that the presence of a non-degenerate companion in the progenitor system could leave an observable trace: a flux excess in the early rise portion of the light curve caused by the ejecta impact with the companion itself. This excess would be observable only under favorable viewing angles, and its intensity depends on the nature of the companion. We searched for the signature of a non-degenerate companion in three years of Supernova Legacy Survey data by generating synthetic light curves accounting for the effects of shocking and comparing true and synthetic time series with Kolmogorov-Smirnov tests. Our most constraining result comes from noting that the shocking effect is more prominent in the rest-frame B than V band: we rule out a contribution from white dwarf-red giant binary systems to Type Ia supernova explosions greater than 10% at the 2Ï, and greater than 20% at the 3Ï level.}
}

